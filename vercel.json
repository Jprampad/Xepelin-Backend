{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/login",
            "dest": "main.py"
        },
        {
            "src": "/api/tasas",
            "dest": "main.py"
        },
        {
            "src": "/api/tasas/(.*)",
            "dest": "main.py"
        },
        {
            "src": "/docs",
            "dest": "main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 